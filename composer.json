{
    "name": "behamin/bresources",
    "description": "Behamin basic and standard api response format for laravel",
    "homepage": "https://github.com/omalizadeh/bresources",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "behamin",
        "request",
        "resource",
        "laravel",
        "package",
        "response",
        "json",
        "collection"
    ],
    "authors": [
        {
            "name": "Omid Alizadeh",
            "email": "om_alizadeh@yahoo.com"
        },
        {
            "name": "Hossein Ebrahimzadeh",
            "email": "abi.hossein@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || >=8.0",
        "ext-json": "*",
        "laravel/framework": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Behamin\\BResources\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "^v6.27.1 || ^7.24 || ^8.5 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Behamin\\BResources\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Behamin\\BResources\\BResourceServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
